  <step>
    <name>Last / First Name Split</name>
    <type>FieldSplitter</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <splitfield>Player</splitfield>
    <delimiter>$[0A]</delimiter>
    <enclosure/>
    <fields>
      <field>
        <name>last_name</name>
        <id/>
        <idrem>N</idrem>
        <type>String</type>
        <format/>
        <group/>
        <decimal/>
        <currency/>
        <length>-1</length>
        <precision>-1</precision>
        <nullif/>
        <ifnull/>
        <trimtype>both</trimtype>
      </field>
      <field>
        <name>first_name</name>
        <id/>
        <idrem>N</idrem>
        <type>String</type>
