  <step>
    <name>All Stats Player Names</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>Oracle Docker Baseball</connection>
    <sql>with player_name as
(
    select coalesce(sbs.last_name, sps.last_name, sfs.last_name)  last_name 
          ,coalesce(sbs.first_name, sps.first_name, sfs.first_name)  first_name
      from c##baseball.stg_batting_stat  sbs
      full outer join c##baseball.stg_pitching_stat  sps
        on sbs.last_name = sps.last_name
       and sbs.first_name = sps.first_name
      full outer join c##baseball.stg_fielding_stat  sfs
        on sbs.last_name = sfs.last_name
       and sbs.first_name = sfs.first_name
)   
select last_name
      ,first_name
  from player_name
 group by last_name
      ,first_name 
 order by last_name
      ,first_name
</sql>
    <limit>0</limit>
